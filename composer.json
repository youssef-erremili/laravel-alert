{
    "name": "errehub/laravel-alert",
    "description": "This project aims to give developers a new experience with alerts in Laravel.",
    "type": "package",
    "license": "MIT",
    "keywords": ["alert", "laravel", "notification", "message", "flash", "session", "toast"],
    "autoload": {
        "psr-4": {
            "Errehub\\LaravelAlert\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Youssef Erremili",
            "email": "yousseferremili19@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Errehub\\LaravelAlert\\AlertServiceProvider"
            ],
            "aliases": {
                "Alert": "Errehub\\LaravelAlert\\AlertFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "illuminate/support": "^10.0"
    }
}
