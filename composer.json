{
    "name": "youssef-erremili/laravel-alert",
    "description": "This project aims to give developers a new experience with alerts in Laravel.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "YoussefErremili\\LaravelAlert\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ],
        "exclude-from-classmap": [
            "vendor/league/flysystem-local/*.php"
        ]
    },
    "authors": [
        {
            "name": "youssef-erremili",
            "email": "yousseferremili19@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "YoussefErremili\\Alert\\AlertServiceProvider"
            ],
            "aliases": {
                "Alert": "YoussefErremili\\Alert\\AlertFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "illuminate/support": "^10.0"
    }
}